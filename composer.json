{
    "name": "bithoven/tickets",
    "description": "Support ticket system for Bithoven framework",
    "type": "library",
    "keywords": ["bithoven", "tickets", "support", "helpdesk", "customer-service"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bithoven Team",
            "email": "dev@bithoven.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^11.0",
        "illuminate/database": "^11.0",
        "illuminate/http": "^11.0",
        "illuminate/routing": "^11.0",
        "illuminate/validation": "^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Bithoven\\Tickets\\": "src/",
            "Bithoven\\Tickets\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bithoven\\Tickets\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bithoven\\Tickets\\TicketsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
