{
    "name": "bithoven/tickets",
    "description": "Support ticket system for Bithoven framework",
    "type": "library",
    "version": "1.3.0",
    "keywords": ["bithoven", "tickets", "support", "helpdesk", "customer-service"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bithoven Team",
            "email": "dev@bithoven.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^11.0",
        "illuminate/database": "^11.0",
        "illuminate/http": "^11.0",
        "illuminate/routing": "^11.0",
        "illuminate/validation": "^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Bithoven\\Tickets\\": "src/",
            "Bithoven\\Tickets\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bithoven\\Tickets\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bithoven\\Tickets\\TicketsServiceProvider"
            ]
        },
        "bithoven": {
            "extension": true,
            "name": "Tickets",
            "description": "Complete support ticket system with categories, assignments, notifications, templates, automation, and DataTables",
            "version": "1.3.0",
            "author": "Bithoven Team",
            "core_version": "^1.4.0",
            "modules": [
                "user-management",
                "authentication",
                "roles-permissions"
            ],
            "permissions": [
                "extensions:tickets:base:view",
                "extensions:tickets:base:create",
                "extensions:tickets:base:edit",
                "extensions:tickets:base:assign",
                "extensions:tickets:categories:manage",
                "extensions:tickets:templates:manage",
                "extensions:tickets:automation:manage",
                "extensions:tickets:sla:manage"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
